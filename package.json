{
  "name": "@dmarfin/jest-browser-reporter",
  "version": "1.0.0",
  "description": "A beautiful browser-based test results reporter for Jest Lite",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "browser": "dist/umd/index.js",
  "types": "dist/types/index.d.ts",
  "unpkg": "dist/index.umd.js",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "set NODE_ENV=production&rollup -c",
	"build-dev": "set NODE_ENV=development&rollup -c",
    "dev": "set NODE_ENV=development&rollup -c -w",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "jest",
    "reporter",
    "browser",
    "test",
    "results",
    "jest-lite"
  ],
  "author": "Dmitriy Marfin",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "rollup": "^2.79.2",
    "rollup-plugin-copy": "^3.5.0",	
    "rollup-plugin-delete": "^3.0.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "typescript": "^5.9.2"
  }
}
